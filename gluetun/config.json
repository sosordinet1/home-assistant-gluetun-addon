{
  "name": "Gluetun",
  "version": "1.0.0",
  "slug": "gluetun",
  "description": "Gluetun VPN client",
  "url": "https://github.com/sosordinet1/home-assistant-gluetun-addon",
  "icon": "/gluetun/icon.png",
  "logo": "/gluetun/logo.png",
  "startup": "services",
  "boot": "auto",
  "options": {
    "VPN_SERVICE_PROVIDER": "nordvpn",
    "OPENVPN_USER": "your_email@example.com",
    "OPENVPN_PASSWORD": "your_password"
  },
  "schema": {
    "VPN_SERVICE_PROVIDER": "str",
    "OPENVPN_USER": "str",
    "OPENVPN_PASSWORD": "str"
  },
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "ports": {
    "9117/tcp": 9117,
    "9091/tcp": 9091,
    "51413/tcp": 51413,
    "51413/udp": 51413
  },
  "cap_add": ["NET_ADMIN"],
  "host_network": true
}
